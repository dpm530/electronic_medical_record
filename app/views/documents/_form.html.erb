<%= form_for([@document.patient, @document], :html => {multipart: true}) do |f| %>

   <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
   </div>

   <div class="field">
      <%= f.label :user_id %>
      <%= f.select :user_id, @users.collect{|p| [p.first_name, p.id]}, { include_blank: true }%>
   </div>


   <div class="field">
      <%= f.label :file %>
      <%= f.file_field :file, multiple: true %>
   </div>

   <div class="actions">
      <%= f.submit %>
   </div>

<% end %>
