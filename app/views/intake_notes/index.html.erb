        <td><%= intake_note.substance_use %></td>
        <td><%= intake_note.family_history %></td>
        <td><%= intake_note.social_history %></td>
        <td><%= intake_note.development_history %></td>
        <td><%= intake_note.educational_occupational_history %></td>
        <td><%= intake_note.legal_history %></td>
        <td><%= intake_note.strength_limitations %></td>
        <td><%= intake_note.other_information %></td>
        <td><%= intake_note.diagnosis_description %></td>
        <td><%= intake_note.diagnosis_justification %></td>
        <td><%= intake_note.clinician_signature %></td>
        <td><%= link_to 'Show', intake_note %></td>
        <td><%= link_to 'Edit', edit_intake_note_path(intake_note) %></td>
        <td><%= link_to 'Destroy', intake_note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

