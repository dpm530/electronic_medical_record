      <th>Educational occupational history</th>
      <th>Legal history</th>
      <th>Strength limitations</th>
      <th>Other information</th>
      <th>Diagnosis description</th>
      <th>Diagnosis justification</th>
      <th>Clinician signature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intake_notes.each do |intake_note| %>
      <tr>
        <td><%= intake_note.user %></td>
        <td><%= intake_note.patient %></td>
        <td><%= intake_note.date %></td>
        <td><%= intake_note.time %></td>
        <td><%= intake_note.duration %></td>
        <td><%= intake_note.location %></td>
        <td><%= intake_note.participants %></td>
        <td><%= intake_note.presenting_problems %></td>
        <td><%= intake_note.general_appearance %></td>
        <td><%= intake_note.dress %></td>
        <td><%= intake_note.motor_activity %></td>
        <td><%= intake_note.insight %></td>
        <td><%= intake_note.judgement %></td>
        <td><%= intake_note.affect %></td>
        <td><%= intake_note.mood %></td>
