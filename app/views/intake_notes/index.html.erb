        <td><%= intake_note.attention_concentration %></td>
        <td><%= intake_note.thought_content %></td>
        <td><%= intake_note.perception %></td>
        <td><%= intake_note.flow_of_thought %></td>
        <td><%= intake_note.interview_behavior %></td>
