<h1>Intake Notes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Patient</th>
      <th>Date</th>
      <th>Time</th>
      <th>Duration</th>
      <th>Location</th>
      <th>Participants</th>
      <th>Presenting problems</th>
      <th>General appearance</th>
      <th>Dress</th>
      <th>Motor activity</th>
      <th>Insight</th>
      <th>Judgement</th>
      <th>Affect</th>
