<div class="container-fluid py-2">
   <div class="container-fluid bg-white border py-4">
      <div class="row">
         <div class="col-md-12">
            <h5>Edit Treatment Plan:</h5>
         </div>
      </div>
      <% if @treatment_plan.errors.full_messages.any? %>
         <% @treatment_plan.errors.full_messages.each do |error_message| %>
            <%= error_message %> <br />
         <% end %>
      <% end %>
      <br>
      <div class="row">
         <div class="col-md-12">
            <%= render 'form', treatment_plan: @treatment_plan %>
         </div>
      </div>
   </div>
</div>
